@model Hands.ViewModels.Models.Graph.Graph
@{
    ViewBag.Title = "Graphs";
    Layout = null;
}

<style>

    table.responsive-table thead {
        line-height: 3em;
        background: #eee;
    }

    .table-bordered {
        border: 0px solid #f4f5f8 !important;
    }

    .table-scroll {
        position: relative;
        width: 100%;
        z-index: 1;
        margin: auto;
        overflow: auto;
        height: 335px;
        overflow-x: hidden;
    }

        .table-scroll table {
            width: 100%;
            min-width: auto;
            margin: auto;
        }

    .table-wrap {
        position: relative;
    }

    .table-scroll th,
    .table-scroll td {
        vertical-align: top;
    }

    .table-scroll thead th {
        position: sticky;
        top: 0;
    }
    /* safari and ios need the tfoot itself to be position:sticky also */
    .table-scroll tfoot,
    .table-scroll tfoot th,
    .table-scroll tfoot td {
        position: -webkit-sticky;
        position: sticky;
        bottom: 0;
        background: #1183e1;
        color: #fff;
        z-index: 4;
    }

    th {
        border: none !important;
    }

    tfoot {
        text-align: center;
        color: white;
        font-weight: 500;
    }

    tbody {
        text-align: center;
    }

    thead {
        text-align: center;
    }

    .text {
        color: #fff;
        background-color: #5867dd;
        text-align: center;
        padding: 5px;
        width: 100%;
        margin: 1px 0px;
        font-size: 15px;
    }

    .text1 {
        color: #fff;
        background-color: #5867dd;
        text-align: center;
        padding: 5px;
        width: 14.5%;
        margin: 1px 1px;
        font-size: 15px;
    }

    #customers {
        font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

        #customers td, #customers th {
            border: 1px solid #ddd;
            padding: 8px;
        }

        #customers tr:nth-child(even) {
            background-color: #f2f2f2;
        }

        #customers tr:hover {
            background-color: #ddd;
        }

        #customers th {
            padding-top: 12px;
            padding-bottom: 12px;
            text-align: left;
            background-color: #4CAF50;
            color: white;
        }

    h1, h3 {
        font-size: 17px;
        border-bottom: 1px solid #d2d2da;
        padding-bottom: 11px;
    }


    .input-group > .custom-file, .input-group > .custom-select, .input-group > .form-control {
        position: relative;
        flex: 1 1 auto;
        width: fit-content !important;
        margin-bottom: 0;
    }
    .csvexport {
        height: 36px;
        margin-top: 8px;
    }

    .csvexport2 {
        margin-top: -42%;
        margin-left: 490px;
    }
    
</style>
